{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "diffsheets.com" }],
      "destination": "https://www.diffsheets.com/:path*",
      "permanent": true
    }
  ]
}
